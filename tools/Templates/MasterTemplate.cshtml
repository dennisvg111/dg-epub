@model MasterModel
@using DG.Epub.CodeGeneration.Models
@using DG.Epub.CodeGeneration.Models.Schema
@using RazorLight
@inherits TemplatePage<MasterModel>
@{
    DisableEncoding = true;
}

//----------------------
// <auto-generated>
//     Generated using @(typeof(BaseSchema).Assembly)
// </auto-generated>
//----------------------

using System.Xml.Serialization;

namespace @(Model.Namespace)
{
    @foreach (var schema in Model.Schema)
    {
        if (schema.Value is ObjectSchema)
        {
            var classModel = new ObjectClassModel()
            {
                ClassSchema = (ObjectSchema)schema.Value,
                Name = schema.Key,
                SchemaMap = Model.Schema
            };

            await IncludeAsync("Templates.ObjectSchema.cshtml", classModel);
        }
    }
}